{
  "expo": {
    "name": "LexyVoz",
    "slug": "LexyVoz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "LexyVoz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {},
      "bundleIdentifier": "com.lexyvoz.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.lexyvoz.app",
      "config": {
        "googleMaps": {
          "apiKey": "$EXPO_PUBLIC_GOOGLE_PLACES_API_KEY_ANDROID"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "googlePlacesApiKeyAndroid": "$EXPO_PUBLIC_GOOGLE_PLACES_API_KEY_ANDROID",
      "googlePlacesApiKeyWeb": "$EXPO_PUBLIC_GOOGLE_PLACES_API_KEY_WEB",
      "eas": {
        "projectId": "ecf70396-2b4b-4884-8b08-dc8033cd051f"
      }
    }
  }
}
